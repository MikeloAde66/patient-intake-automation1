            "errorMessage": "={{ $json.errorMessage }}",
            "patientEmail": "={{ $json.patientEmail }}",
            "patientName": "={{ $json.patientName }}",
            "stackTrace": "={{ $json.stackTrace }}",
            "rawData": "={{ $json.rawData }}"
          }
        },
        "options": {}
      },
      "id": "log-error",
