{
  "name": "Patient Data Entry Automation v1.0",
  "nodes": [
    {
      "parameters": {
        "httpMethod": "POST",
        "path": "patient-intake",
        "responseMode": "responseNode",
        "options": {}
      },
      "id": "webhook-trigger",
      "name": "Patient Form Webhook",
      "type": "n8n-nodes-base.webhook",
      "typeVersion": 1.1,
      "position": [240, 300]
    },
    {
      "parameters": {
        "jsCode": "// Generate unique patient ID\nconst timestamp = Date.now();\nconst random = Math.floor(Math.random() * 9999).toString().padStart(4, '0');\nconst patientId = `PT${timestamp}${random}`;\n\n// Get and sanitize form data\nconst body = $input.item.json.body || $input.item.json;\n\nconst patient = {\n  patientId,\n  firstName: (body.firstName || '').trim(),\n  lastName: (body.lastName || '').trim(),\n  dateOfBirth: body.dateOfBirth || '',\n  email: (body.email || '').toLowerCase().trim(),\n  phone: (body.phone || '').replace(/\\D/g, ''),\n  address: (body.address || '').trim(),\n  city: (body.city || '').trim(),\n  state: (body.state || '').trim(),\n  zipCode: (body.zipCode || '').trim(),\n  emergencyContact: (body.emergencyContact || '').trim(),\n  emergencyPhone: (body.emergencyPhone || '').replace(/\\D/g, ''),\n  medicalHistory: body.medicalHistory || '',\n  allergies: body.allergies || 'None reported',\n  currentMedications: body.currentMedications || 'None reported',\n  insuranceProvider: body.insuranceProvider || '',\n  insuranceId: (body.insuranceId || '').trim(),\n  submissionDate: new Date().toISOString(),\n  status: 'New - Pending Review'\n};\n\n// Validation\nif (!patient.firstName || !patient.lastName || !patient.email) {\n  throw new Error('Missing required fields: firstName, lastName, and email are required');\n}\n\n// Email format check\nconst emailRegex = /^[^\\s@]+@[^\\s@]+\\.[^\\s@]+$/;\nif (!emailRegex.test(patient.email)) {\n  throw new Error('Invalid email format');\n}\n\nreturn { json: patient };"
      },
      "id": "process-data",
      "name": "Generate ID & Validate",
      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
      "position": [460, 300]
    },
    {
      "parameters": {
        "authentication": "oAuth2",
        "operation": "append",
        "documentId": {
          "__rl": true,
          "value": "={{ $env.GOOGLE_SHEET_ID }}",
          "mode": "id"
        },
        "sheetName": {
          "__rl": true,
          "value": "Patient_Records",
          "mode": "name"
        },
        "columns": {
          "mappingMode": "defineBelow",
          "value": {
            "patientId": "={{ $json.patientId }}",
            "firstName": "={{ $json.firstName }}",
            "lastName": "={{ $json.lastName }}",
            "dateOfBirth": "={{ $json.dateOfBirth }}",
            "email": "={{ $json.email }}",
            "phone": "={{ $json.phone }}",
            "address": "={{ $json.address }}",
            "city": "={{ $json.city }}",
            "state": "={{ $json.state }}",
            "zipCode": "={{ $json.zipCode }}",
            "emergencyContact": "={{ $json.emergencyContact }}",
            "emergencyPhone": "={{ $json.emergencyPhone }}",
            "medicalHistory": "={{ $json.medicalHistory }}",
            "allergies": "={{ $json.allergies }}",
            "currentMedications": "={{ $json.currentMedications }}",
            "insuranceProvider": "={{ $json.insuranceProvider }}",
            "insuranceId": "={{ $json.insuranceId }}",
            "submissionDate": "={{ $json.submissionDate }}",
            "status": "={{ $json.status }}"
          }
        },
        "options": {}
      },
      "id": "log-to-sheets",
      "name": "Log to Google Sheets",
      "type": "n8n-nodes-base.googleSheets",
      "typeVersion": 4.4,
      "position": [680, 300],
      "credentials": {
        "googleSheetsOAuth2Api": {
          "id": "GOOGLE_SHEETS_CREDENTIAL_ID",
          "name": "Google Sheets OAuth2"
        }
